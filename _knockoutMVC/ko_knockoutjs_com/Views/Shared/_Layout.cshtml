   <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
         <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
               <span class="icon-bar"></span>
               <span class="icon-bar"></span>
               <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("knockoutjs.com", "_1_Hello", "ko", new { area = "" }, new { @class = "navbar-brand" })
         </div>
         <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
               <li class="dropdown">
                  <a tabindex="0" data-toggle="dropdown">KO first examples<span class="caret"></span></a>
                  <ul class="dropdown-menu" role="menu">
                     <li>@Html.ActionLink("Hello", "_1_Hello", "ko")</li>
                     <li>@Html.ActionLink("Licznik klikniec", "_2_LicznikKlikniec", "ko")</li>
                     <li>@Html.ActionLink("Prosta lista", "_3_ProstaLista", "ko")</li>
                     <li>@Html.ActionLink("Lista zwykła", "_4_Lista", "ko")</li>
                     <li>@Html.ActionLink("Kontrola Typów", "_5_KontrolaTypow", "ko")</li>
                     <li>@Html.ActionLink("Kolekcje", "_6_Kolekcje", "ko")</li>
                     <li>@Html.ActionLink("Tabela Ze Stronicowaniem", "_7_TabelaZeStronicowaniem", "ko")</li>
                     <li>@Html.ActionLink("Animowane Przejscia", "_8_AnimowanePrzejscia", "ko")</li>
                     <li>@Html.ActionLink("Test_KO", "Test_KO", "ko")</li>
                  </ul>
               </li>
               <li class="dropdown">
                  <a tabindex="0" data-toggle="dropdown">Detailed examples<span class="caret"></span></a>
                  <ul class="dropdown-menu" role="menu">
                     <li><a href="/Detailed/_1_ContactEditor">Contact editor</a></li>
                     <li>@Html.ActionLink("Test_KO", "Test_KO", "Detailed")</li>
                  </ul>
               </li>
               <li class="dropdown">
                  <a tabindex="0" data-toggle="dropdown">Templates<span class="caret"></span></a>
                  <ul class="dropdown-menu" role="menu">
                     <li>@Html.ActionLink("Named template", "_01_NamedTemplate", "Templates")</li>
                     <li>@Html.ActionLink("Foreach option with Named template", "_02_NamedTempWithForeach", "Templates")</li>
                     <li><a href="/Templates/_03_UsingASWithForeach">Using AS With Foreach items</a></li>
                     <li><a href="/Templates/_04_Using_afterRender_afterAdd_beforeRemove">Using afterRender, afterAdd, beforeRemove</a></li>
                     <li><a href="/Templates/_05_DinamicallyChoosingTemplate">Dinamically Choosing Template</a></li>
                     <li><a href="/Templates/_06_jQuery_tmpl">Using jQuery.tmpl</a></li>
                     <li><a href="/Templates/_07_Underscore_js_template"> Underscore.js template</a></li>


                     <li>@Html.ActionLink("Test Template", "Test_Template", "Templates")</li>
                  </ul>
               </li>
            </ul>
         </div>
      </div>
   </div>

   <div class="container body-content">
      @RenderBody()
      <hr />
      <footer>
         <p>&copy; @DateTime.Now.Year - <a href="http://knockoutjs.com">knockoutjs.com</a></p>
      </footer>
   </div>

   @Scripts.Render("~/bundles/jquery")
   @Scripts.Render("~/bundles/bootstrap")
   @RenderSection("scripts", required: false)

</body>
</html>





